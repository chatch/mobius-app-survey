{
  "name": "survey-rewards-mobius-client",
  "version": "0.0.2",
  "description": "Survey Rewards App Client",
  "license": "MIT",
  "repository": "chatch/mobius-app-survey",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender",
    "serve": "npm run build && preact serve",
    "dev": "preact watch",
    "deploy": "up",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "react/sort-comp": "off",
      "react/prefer-stateless-function": "off",
      "no-unused-vars": "warn",
      "lines-around-comment": "off",
      "indent": [
        "error",
        2
      ],
      "react/jsx-no-bind": "warn",
      "react/jsx-indent-props": [
        2,
        2
      ],
      "semi": "off",
      "no-console": "off"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.0"
  },
  "dependencies": {
    "node-fetch": "^2.1.2",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-material-components": "^1.4.3",
    "preact-router": "^2.5.5",
    "react-md-spinner": "^0.3.0",
    "react-responsive-modal": "^3.2.0",
    "react-table": "^6.8.6",
    "survey-react": "^1.0.32",
    "surveyjs-editor": "^1.0.32"
  }
}
